<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.abhiuday.networkinglibrarytesting" >
    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/NoActionBarTheme" >
        <activity
            android:name=".retrofit.RetrofitActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />
                <!--<data android:host="NetworkLibraryTest" android:scheme="networkingdemo.com" />-->
                <data android:scheme="networkingdemo" android:host="oauthResponse" />
                <!--<data android:scheme="networkingdemo.com/ok" android:host="networkingdemo" />-->
            </intent-filter>
        </activity>
    </application>

</manifest>
